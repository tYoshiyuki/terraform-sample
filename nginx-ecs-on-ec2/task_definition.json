{
  "family": "sample-ecs-task",
  "containerDefinitions": [
      {
          "name": "nginx",
          "image": "nginx:1.14",
          "cpu": 0,
          "portMappings": [
              {
                  "containerPort": 80,
                  "hostPort": 0,
                  "protocol": "tcp"
              }
          ],
          "essential": true
      }
  ],
  "taskRoleArn": "${task_role}",
  "executionRoleArn": "${task_execution_role}",
  "networkMode": "bridge",
  "requiresCompatibilities": [
      "EC2"
  ],
  "cpu": "256",
  "memory": "512"
}